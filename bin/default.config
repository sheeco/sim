simulation.slot 1
simulation.datatime 30000
simulation.runtime 35000
simulation.routing_protocol 3

log.slot_log 50
log.slot_brief 100
log.detail 3

trace.path ../res/NCSU-
trace.continuous true
sink.x 0
sink.y 0

node.default_data_rate 6.8
node.buffer 20

trans.speed 2500
trans.range 100
trans.probability 1.0
trans.size_window 10

mac.cycle 30
mac.duty_rate 1.0
mac.sync_cycle true

ma.buffer 200
ma.init_num 5

hs.alpha 0.03
hs.test_hotspot_similarity true
har.beta 0.0025
har.heat_1 1
har.heat_2 30

prophet.init_pred 0.70
prophet.decay_pred 0.90
prophet.trans_strict_by_pred true

pferry.path_predict ../res/lstm
pferry.keyword_predict .test
pferry.waiting_time 30

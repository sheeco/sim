simulation.slot 1
simulation.datatime 15000
simulation.runtime 15000
simulation.routing_protocol 1

log.slot_log 100

trace.path ../res/NCSU-
trace.continuous true
sink.x 200
sink.y 200

node.default_data_rate 6.8
node.buffer 20

trans.speed_trans 2500
trans.range_trans 250
trans.prob_trans 1.0
trans.window_trans 10

mac.cycle 30
mac.duty_rate 1.0
mac.sync_cycle true

ma.buffer 100
ma.init_num_ma 5
ma.max_num_ma 5

hs.alpha 0.03
hs.test_hotspot_similarity true
har.beta 0.0025
har.heat_1 1
har.heat_2 30

prophet.init_pred 0.70
prophet.decay_pred 0.90
prophet.trans_strict_by_pred true

pferry.path_predict ../res/lstm
pferry.keyword_predict .full
